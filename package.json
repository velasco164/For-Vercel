{
  "name": "velascoquizgame",
  "version": "1.0.0",
  "description": "Backend server for quiz game",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"cd src/server && ts-node server.ts\"",
    "build": "tsc --jsx react-jsx",
    "start": "cd src/server && node dist/server.js",
    "serve": "cd src/server && npm run build && npm start"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^7.0.0",
    "pg": "^8.11.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.6"
  }
}
